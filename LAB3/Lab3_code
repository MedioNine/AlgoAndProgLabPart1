#include <stdio.h>
#include <math.h>

int factorial (int f)
{
    int jojo = 1;
    int i;
    for(i = 1; i<=f; i++);
    jojo = jojo * i; 
    return jojo;
}

int main ()
{
    double summa = 1.0, summa_e = 1.0, e = 0.0001;
     double k;
 for ( double x = 0.1; x<=1.0; x +=(1.0-0.1)/10.0)
      {
         for (int n = 1.0; n<=10.0; n++)
         {
             k = (pow(-1,n) * pow(x,2*n))/factorial(2*n);
             if (k>e || k<e) 
                 summa_e += k;
             summa += k;
         }
     printf ("X=%.1f SN=%f SE=%.4f Y=%f\n", x, summa, summa_e, cos(x));
      }
}
