#include <stdio.h>
#include <string.h>

int main(){
    char a[255];
    int i = 0;
    int  j,min,c;
    //ввід рядка
    do {
        a[i] = getchar();
        i++;
    }
    while (a[i-1]!='\n');
    int len = strlen(a);
    //сортування рядка відносно коду ASCII
    for(i = 0; i<len-1; i++)
    { 
        min = i;
        for (j = i+1; j<len-1; j++)
        {
            if (a[j]<a[min])
                min = j;      
        }
        if (min!=i) 
        {
            c = a[i];
            a[i] = a[min];
            a[min] = c; 
        }
     }
    for (i = 0; i<len-1; i++)
        printf ("%c", a[i]);
printf("\n");
}
