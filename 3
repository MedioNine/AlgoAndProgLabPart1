#include <stdio.h>

int main(){
    int N_A,N_B;
    int A[10], B[10];
printf("Numbers of A: ");
    scanf("%d",&N_A);
    for (int i = 0; i<N_A; i++)
        scanf("%d",&A[i]);
    printf("Numbers of B: ");
    scanf("%d",&N_B);
        for (int i = 0; i<N_B; i++)
        scanf("%d",&B[i]);
    for (int i = 0; i<N_A;i++){
        for (int j = 0; j<N_B; j++)
            if (A[i]<3*B[j]) printf("1 "); else printf ("0 "); 
    printf("\n");}
}
